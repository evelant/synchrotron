{
	"name": "synchrotron",
	"private": true,
	"author": {
		"name": "Andrew Morsillo",
		"url": "https://github.com/evelant/synchrotron"
	},
	"bugs": {
		"url": "https://github.com/evelant/synchrotron/issues"
	},
	"description": "A ",
	"homepage": "https://github.com/evelant/synchrotron",
	"license": "MIT",
	"version": "0.0.1",
	"type": "module",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "pnpm run dev:web",
		"dev:web": "pnpm --filter @synchrotron-examples/todo-app-web-pglite run dev",
		"dev:react-native": "pnpm --filter todo-app-react-native-sqlite run dev",
		"dev:backend": "pnpm --filter @synchrotron-examples/backend run dev",
		"test:e2e:postgres": "bun scripts/test-e2e-postgres.ts",
		"docker:up": "pnpm --filter @synchrotron-examples/backend run docker:up",
		"docker:down": "pnpm --filter @synchrotron-examples/backend run docker:down",
		"docker:reset": "pnpm --filter @synchrotron-examples/backend run docker:reset",
		"clean:node_modules": "pnpm run -r clean:node_modules",
		"build": "pnpm run -r build",
		"clean": "rm -rf .tsbuildinfo build dist && pnpm run -r clean",
		"format": "prettier --write .",
		"typecheck": "tsc -b",
		"typecheck:watch": "tsc -b --watch",
		"lint": "prettier --check . && eslint .",
		"test": "vitest run -c vitest.config.ts",
		"test:watch": "vitest -c vitest.config.ts",
		"test:coverage": "vitest run --coverage -c vitest.config.ts",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "~2.29.8",
		"@effect/language-service": "~0.64.1",
		"@types/bun": "~1.3.5",
		"@types/node": "catalog:",
		"blob-polyfill": "~9.0.20240710",
		"eslint": "~9.24.0",
		"eslint-config-prettier": "~10.1.8",
		"prettier": "~3.7.4",
		"typescript": "catalog:",
		"typescript-eslint": "~8.53.0",
		"vite": "~7.3.1",
		"vite-plugin-top-level-await": "~1.6.0",
		"vite-plugin-wasm": "~3.5.0",
		"vite-tsconfig-paths": "~5.1.4",
		"vitest": "catalog:",
		"repomix": "~0.3.1"
	},
	"packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
