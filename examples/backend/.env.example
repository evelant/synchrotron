POSTGRES_PORT='56321'
ADMIN_DATABASE_URL='postgresql://postgres:password@127.0.0.1:56321/electric'
DATABASE_URL='postgresql://synchrotron_app:password@127.0.0.1:56321/electric'
PROJECT_NAME='synchrotron-examples-backend'

# JWT auth for Synchrotron RPC (HS256, Supabase-compatible claim defaults).
# The RPC server requires `Authorization: Bearer <jwt>` and derives `user_id` from `sub`.
SYNC_JWT_SECRET='supersecretvalue-supersecretvalue'
# Optional: verify `aud` in incoming JWTs (Supabase commonly uses `authenticated` / `anon`).
SYNC_JWT_AUD='authenticated'
