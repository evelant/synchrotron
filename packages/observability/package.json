{
	"name": "@synchrotron/observability",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"description": "OpenTelemetry helpers for Synchrotron apps (Effect-native tracing).",
	"exports": {
		".": "./dist/index.js",
		"./node": "./dist/node.js",
		"./web": "./dist/web.js"
	},
	"typesVersions": {
		"*": {
			"*": ["dist/*"]
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc -b tsconfig.build.json",
		"clean": "rm -rf .tsbuildinfo build dist",
		"clean:node_modules": "rm -rf node_modules",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@effect/platform": "catalog:",
		"@effect/opentelemetry": "catalog:",
		"@opentelemetry/api": "catalog:",
		"@opentelemetry/exporter-trace-otlp-http": "catalog:",
		"@opentelemetry/sdk-trace-base": "catalog:",
		"@opentelemetry/sdk-trace-node": "catalog:",
		"effect": "catalog:"
	},
	"devDependencies": {
		"@types/node": "catalog:",
		"typescript": "catalog:"
	}
}
